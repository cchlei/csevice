.font_wei{font-family:"Microsoft YaHei"}
.font_song{font-family:SimSun}
.navright,.navright form.mapupdata .greybg p,.navright form.mapupdata .saveupdate span,.navright form.mapupdata p,.navright form.mapupdata p span,.releaselayer{font-family:"Microsoft YaHei"}
textarea{outline:0;resize:none}
.navright{position:relative;color:#333}
.navright .maptop{height:115px;line-height:115px;background-color:#eaedf2;position:fixed;top:0;width:100%;z-index:999}
.navright .maptop em{position:absolute;font-size:28px;top:0;left:33px}
.navright .maptop span{height:115px;line-height:115px;position:absolute;top:0;right:33px;display:block;font-size:14px;cursor:pointer}
.navright form.mapupdata{margin:195px auto 0}
.navright form.mapupdata p{width:570px;margin:0 auto;line-height:5em;position:relative;font-size:14px;color:#898989}
.navright form.mapupdata p em{width:7em;display:inline-block;text-align:right;padding-right:1em;color:#333}
.navright form.mapupdata p em.fwmsname{position:relative;top:-129px;line-height:2.5em}
.navright form.mapupdata p em.grey{color:#bbbaba;display:block;width:11em;padding-left:9em;text-align:inherit;padding-right:0;line-height:2.5em;position:absolute;top:165px}
.navright form.mapupdata p.biaoqian em{position:relative;top:-40px;line-height:2.5em}
.navright form.mapupdata p.biaoqian span.span{width:28em;display:inline-block;line-height:3em}
.navright form.mapupdata p input,.navright form.mapupdata p textarea{width:24em;font-size:1em;padding:1em}
.navright form.mapupdata p .label,.navright form.mapupdata p input.interfacetype{width:2em;vertical-align:middle;padding:0}
.navright form.mapupdata p label{display:inline-block}
.navright form.mapupdata p .label{margin:0 5px 0 3px}
.navright form.mapupdata p textarea{height:128px;overflow:hidden;margin-top:10px}
.navright form.mapupdata p i{width:16px;height:16px;margin:0 5px;display:-webkit-inline-box;background:url(../images/checkbox.png) center -16px no-repeat}
.navright form.mapupdata p i.ed{background:url(../images/checkbox.png) center 1px no-repeat}
.navright form.mapupdata p span.jqTransformCheckboxWrapper{float:none;display:inline-block;margin:-6px 3px}
.navright form.mapupdata p .errtip.err{display:block;color:red;line-height:2em;padding-left:8em}
.navright form.mapupdata p .webuploader-pick{display:inline;background-color:transparent;color:#0078d7}
.navright form.mapupdata p select{padding:10px 0;border:1px solid #ccc;border-radius:4px;box-shadow:inset 0 1px 1px rgba(0,0,0,.075);font-size:14px}
.navright form.mapupdata p .preview{display:inline-block;margin-top:1em;margin-bottom:0;position:relative}
.navright form.mapupdata p .preview img{width:368px;margin-left:116px}
.navright form.mapupdata p .preview .tip{position:absolute;z-index:8;background-color:rgba(232,69,51,.66);color:#fff;width:90px;height:36px;line-height:36px;text-align:center;left:116px}
.navright form.mapupdata .greybg{width:80%;margin:0 auto 30px;background-color:#f9f8f8}
.navright form.mapupdata .greybg p{width:570px;margin:0 auto;height:54px;line-height:54px;font-size:16px}
.navright form.mapupdata .greybg em{width:100px;text-align:right;padding-right:1em;color:#4c79d6}
.navright form.mapupdata .greybg span{color:#898989;position:absolute;width:800px}
.navright form.mapupdata .saveupdate{width:405px;margin:45px auto 88px}
.navright form.mapupdata .saveupdate span{width:150px;height:40px;line-height:40px;font-size:14px;color:#fff;display:inline-block;text-align:center}
.navright form.mapupdata .saveupdate span.mapsave{background-color:#4c79d6}
.navright form.mapupdata .saveupdate span.maprelease,.navright form.mapupdata .saveupdate span.mapup{background-color:#44c450;margin-left:100px}
.updatebg{width:480px;height:200px;background:url(../images/updatebg.jpg) center no-repeat}
.updatelayer{display:none;width:340px;height:63px;margin:69px auto}
.updatelayer img{display:block;float:left}
.updatelayer .uptishi{float:left;margin-left:20px}
.updatelayer .uptishi h3{font-weight:100;color:#1a9aff;height:30px;font-size:24px}
.updatelayer .uptishi p{font-size:14px;line-height:33px}
.relasebg{width:855px;border-top:1px #4c79d6 solid}
.releaselayer{width:760px;display:none;font-size:14px;color:#262626;font-weight:600;margin:0 auto;padding-top:65px}
.releaselayer h3{font-size:18px;height:25px;line-height:25px}
.releaselayer span{color:#565454;font-weight:100}
.releaselayer p{line-height:2em}
.releaselayer p.agreebutton{text-align:center}
.releaselayer p span.agreement,.releaselayer p span.calcenagree{width:150px;height:40px;line-height:40px;text-align:center;margin:50px auto;color:#fff;display:inline-block}
.releaselayer p span.agreement{background-color:#44c450;margin-left:120px}
.releaselayer p span.calcenagree{background-color:#4c79d6}