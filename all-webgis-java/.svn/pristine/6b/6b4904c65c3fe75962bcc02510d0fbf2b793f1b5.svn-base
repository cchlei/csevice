.gap_vline{
	._icon(1,44,none);
	background-image: url(../images/gap_line.png);
	vertical-align: middle;
	margin: 0 9px;
}



.r1,.r2,.r3,.r4,.r6,.r6,.r7,.r8,.r9,.r10{
	overflow: hidden;
	>.mc{
		height: 100%;
		position: relative;
	}

	.title{
		margin-top: 69px;
		height: 255px;
		display: block;
		&:extend(.__icon);
		background-image: url(../images/tx_r2_title.png);
	}

	p{
		font-size: 20px;
		line-height: 2em;
		color: #fff;
		margin-top: 45px;
		text-align: center;
	}
}


//创建地图按钮
.create_map_btn{
	display: block;
	&:extend(.center);
	._size_lh(250,60);
	font-size: 30px;
	color: #ffffff;
	border: 1px solid #fff;
	margin-top: 100px;
	.bgcolor_alpha();
	&:hover{
		color: #08c;
	}
}

#link_pop{
	.bgcolor_alpha(rgba(97, 197, 255,0.9));
	color: #fff;
	text-decoration: underline;
	padding: 75px 75px;
	a{
		color: #fff;
		padding: 0 18px;
		font-size: 16px;
	}
	display: none;
}

.ly_skin{
	background-color: transparent !important;
}