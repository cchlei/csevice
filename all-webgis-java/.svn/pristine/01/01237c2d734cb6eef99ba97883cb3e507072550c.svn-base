@import "tool";
@import "vars";
.toolClass;
textarea{
  resize: none;
}
.addsubform{
  tr:first-child+tr{
    .switch,.friend{
      display: inline-block;
      vertical-align: middle;
      input,span{
        display: inline-block;
        vertical-align: middle;
      }
      input{
        width: 15px;
        padding: 0px;
        margin: 0px 4px 0px 0px;
        border: 0px;
      }
    }
    .friend{
      margin-left: @g*2;
      label{
        cursor: pointer;
      }
    }
    .layui-layer-title{
      padding: 0 80px 0 12px!important;
      background-color: #5087d6;
      color: #fff;
      font-size: 14px;
      font-family: @font_yahei;
    }
    .layui-layer-ico.layui-layer-close.layui-layer-close1{
      background-position: -80px -40px;
    }
    .layui-layer-btn{
      .layui-layer-btn0,.layui-layer-btn1{
        width: 50px;
        height: 25px;
        line-height: 25px;
        text-align: center;
        border-radius: 5px;
      }
      .layui-layer-btn0{
        background-color: #5087d6;
      }
      .layui-layer-btn1{
        background-color: #b9b9b9
      }
    }
    .zdfriend{
      display: none;
      padding: 12px;
      font-family: @font_yahei;
      color: #444;
      h3{
        line-height: 30px;
        font-size: 14px;
        font-weight: 300;
      }
      .left,.right{
        background-color: #f6f6f6;
        overflow: auto;
        display: inline-block;
      }
      .left{
        width: 188px;
        height: 324px;
        padding-left: 6px;
        h3{
          text-indent: 22px;
          cursor: pointer;
          background: url(../images/flist_jt.png) no-repeat 4px center;
          position: relative;
          &.cur{
            color: #fff;
            background: url(../images/flist_jt1.png) no-repeat 4px center #5087d6;
            i{
              position: absolute;
              right: 6px;
              top: 5px;
              width: 19px;
              height: 19px;
              background: url(../images/flist_add.png) no-repeat;
            }
          }
        }
        ul{
          //display: none;
          li{
            height: 38px;
            line-height: 38px;
            font-size: 13px;
            cursor: pointer;
            margin-top: 1px;
            &.cur{
              background: #fffbcd;
            }
            img{
              width: 26px;
              height: 26px;
              border-radius: 30px;
              display: inline-block;
              vertical-align: middle;
              border: 2px #fff solid;
            }
          }
        }
      }
      .right{
        width: 244px;
        height: 324px;
        margin-left: 20px;
        padding-left: 6px;
        h3{
          text-indent: 0px;
        }
        p{
          position: relative;
          margin-top: 1px;
          &.cur{
            background: #fffbcd;
            i{
              position: absolute;
              right: 6px;
              top: 5px;
              width: 22px;
              height: 22px;
              background: url(../images/flist_yc.png) no-repeat;
              cursor: pointer;
            }
          }
          img{
            width: 26px;
            height: 26px;
            border-radius: 30px;
            display: inline-block;
            vertical-align: middle;
            border: 2px #fff solid;
          }
        }
      }
    }
  }
  td{
    &.header-img{
      width: 234px;
      height: 234px;
      display: inline-block;
      vertical-align: top;
    }
    &.imgupload{
      display: inline-block;
      width: 242px;
      height: 232px;
      vertical-align: top;
      border: 1px solid #ddd;
      padding: 0px;
      margin: 24px 0px 0px 15px;
      h3{
        line-height: 30px;
        background-color: #ececec;
        font-size: 15px;
        font-family: @font_yahei;
        text-align: right;
        margin-right: 6px;
        cursor: pointer;
        i{
          background: url("../images/uploadico.png") no-repeat center 0px;
          display: inline-block;
          width: 26px;
          height: 19px;
          vertical-align: sub;
        }
        &.webuploader-container{
          input.webuploader-element-invisible{
              width: 100px;
          }
        }
        .webuploader-pick{
          padding: 0px !important;
          display: inline-block;
          vertical-align: top;
          color: #3469b4;
        }
      }
      ul{
        height: 190px;
        overflow: auto;
        background-color: #fff;
        padding-top: 11px;
        li{
          display: inline-block;
          vertical-align: top;
          width: 100px;
          height: 100px;
          margin: 0px 0px 9px 9px;
          cursor: pointer;
          position: relative;
          }
          img{
            width: 100px;
            height: 100px;
            border: 1px #fff solid;
            &:hover,&.on{
              position: absolute;
              top: 0px;
              left: 0px;
              border: 1px #ffb400 solid;
          }
        }
      }
    }
  }
}
.addsubform,.record_add{
  position: relative;
  tr{
    display: table-row;
    vertical-align: inherit;
    border-color: inherit;
    &:first-child{
      td{
        padding:@g*3 0px 0px @g*2;
      }
    }
    &:first-child+tr,&:first-child+tr+tr+tr{
      td{
        &:first-child+td {
          vertical-align: middle;
        }
      }
    }
    &:first-child+tr+tr+tr{
      td{
        &:first-child+td{
          span{
            width: 30px;
            height: 30px;
            border-radius: 35px;
            display: inline-block;
            margin-right: 3px;
            border: 1px #e8eff9 solid;
            position: relative;
            text-align: center;
            cursor: pointer;
            &:hover{
              border: 1px #979798 solid;
            }
            &.gray{
              border: 1px #979798 solid;
            }
          }
          em{
            width: 24px;
            height: 24px;
            border-radius: 25px;
            position: absolute;
            top: 3px;
            left: 3px;
            &.co1{
              background-color: #c863d4;
            }
            &.co2{
              background-color: #2bba8d;
            }
            &.co3{
              background-color: #3469b4;
            }
            &.co4{
              background-color: #ffb400;
            }
            &.co5{
              background-color: #333a44;
            }
            &.co6{
              background-color: #ec4a4c;
            }
            &.co7{
              background-color: #93cddd;
            }
            &.co8{
              background-color: #4aa2ec;
            }
            &.co9{
              background-color: #f952a1;
            }
            &.co10{
              background-color: #43ab19;
            }
            &.co11{
              background-color: #cfec4a;
            }
            &.co12{
              background-color: #ae0505;
            }
          }
        }
      }
    }
    td{
      vertical-align: top;
      padding:@g*2 0px 0px @g*2;
      .xinst();
      position: relative;
      line-height: 20px;
      height: 20px;
      vertical-align: middle;
      &.linkadress{
        i{
          width: 58px;
          height: 20px;
          display: inline-block;
          background: url("../images/icon_theme13.png") no-repeat center 0px;
          vertical-align: middle;
          &.link1{
            background: url("../images/icon_theme12.png") no-repeat center 0px;
          }
        }
      }
      em{
        width: 100px;
        height: 32px;
        line-height: 32px;
        display: inline-block;
        text-align: center;
        font-size: 16px;
        .xinst();
        border-radius: 5px;
        background: @cl_orange;
        color: #fff;
      }
      input,textarea{
        width: 480px;
        font-size: 14px;
        border: 1px #ddd solid;
        border-radius: 5px;
        padding: 5px;
        color: #666;
      }
      input{
        height: 20px;
        line-height: 20px;
        &[name=addrname]{
          width: 350px;
        }
      }
      textarea{
        height: 105px;
      }
      .prompt{
        width: 474px;
        position: absolute;
        z-index: 9;
        background-color: #edeaee;
        border: 1px #efd9d9 solid;
        opacity: 0.9;
        padding: 15px 8px 20px;
        display: none;
        span{
          position: relative;
          padding: 6px 8px;
          background-color: #ec4a4c;
          display: inline-block;
          border-radius: 5px;
          cursor: pointer;
          margin-top: 5px;
          margin-right: 5px;
          color: #fff;
          opacity: 1;
        }
        .close{
          position: absolute;
          right: 2px;
          top: 3px;
          font-size: 14px;
          color: #333;
          cursor: pointer;
        }
      }
      .switch{
        width: 80px;
        height: 30px;
        border-radius: 30px;
        background-color: #f0e3e3;
        color: #ec4a4c;
        cursor: pointer;
        span{
          width: 36px;
          height: 24px;
          &:extend(.yahei);
          font-size: 14px;
          line-height: 24px;
          text-align: center;
          background: none;
          display: inline-block;
          vertical-align: top;
          margin: 3px 0 0 7px;
          &.close{
            background-color: #ec4a4c;
          }
          &.open,&.close{
            width: 24px;
            height: 24px;
            border-radius: 25px;
          }
        }
        &.blue{
          background-color: #d3e0f2;
          span{
            color:#3469b4;
            &.open{
              background-color: #3469b4;
            }
          }
        }
      }
      .sub_cancel,.sub_save{
        width: 80px;
        .xinst();
        font-size: 14px;
        border-radius: 0.65em + 0.9;
        text-align: center;
        color: #fff;
        display: inline-block;
        .padding_v(0.5em);
        cursor: pointer;
      }
      .sub_cancel{
        background-color: #333a44;

      }
      .sub_save{
        background-color: #ec4a4c;
        margin-left: @g;
      }
      &.header-img,.uploadthing{
        input[name='file']{
          width: 223px;
        }
        .webuploader-pick{
          padding: 0px;
          img{
            width: 234px;
          }
        }
      }

    }
    &.bth{
      position: absolute;
      top: -48px;
      right:@g*4;
      td{
        padding: 0px;
      }
    }
  }
}
.errtip.err{
  color: #ec4a4c;
  margin-left: 12px;
  display: inline-block;
}

.mainCont {
  .page_name {
    .ico_mytheme,.ico_record{
      display: inline-block;
      margin-right: 6px;
      vertical-align: middle;
    }
    .ico_mytheme {
      width: 16px;
      height: 20px;
      background: url("../images/icon_theme10.png") no-repeat;
    }
    .ico_record{
      width: 16px;
      height: 23px;
      background: url("../images/icon_theme11.png") no-repeat;
    }

  }
}
.page_name.white{
   background-color: #fff;
}

.record_add{
  td{
    &.webup{
      padding: @g*3 0 0 @g*4 !important;
    }
    .occurtime{
      width: 230px;
      padding: 5px;
      border-radius: 5px;
      border: 1px solid #ddd;
    }
    .errtip{
      padding: 12px 0px 0px;
    }
    .uploadthing{
      width: 480px;
      height: 272px;
      margin-top: @g*2;
      background-color: #fff;
      border: 1px #ddd solid;
      overflow: auto;
      padding-bottom: 10px;
      .file{
        position: relative;
        margin: 10px 0px 20px 10px;
        display: inline-block;
        i{
          width: 100px;
          text-align: center;
          position: absolute;
          bottom: 0;
          left: 0;
          color: #fff;
          background-color: #333;
          cursor: pointer;
        }
        img{
          width: 100px;
          height: 100px;
          cursor: pointer;
        }
        p{
          width: 100px;
          text-align: center;
          position: absolute;
          bottom: -20px;
          height:18px;
          overflow: hidden;
          display: block;
          white-space: nowrap;
          -o-text-overflow: ellipsis;
          text-overflow:  ellipsis;
        }
      }
    }
    .uploadclick{
      cursor: pointer;
      .webuploader-pick{
        padding: 0px;
      }
    }
  }
}
.map{
  margin-left: @g*2;
  margin-top: @g*2;
  width:1147px !important;
  height: 235px !important;
}
.map2{
  margin-left: @g*2;
  margin-top: @g*2;
  width: 645px;
}
.cover_img{
  width: 234px;
  height: 234px;
}






.map_wrapper{
  margin-top:@g * 2;
  margin-left:@g * 2;
  position: relative;
  width: 1145px;
  #mapel{
    height: 330px;
    background-color: #333;
  }

  .btn_bar{
    position: absolute;
    background-color: #ffffff;
    z-index: 9;

    right:@g;
    top:@g;
    font-size: 0;

    >a{
      display: inline-block;
      white-space: nowrap;
      color: @cl_blue;
      border: 1px  solid @cl_blue;
      padding:0 @g;
      line-height:2.5 * @g;
      cursor: default;
      font-size: @f;
      border-right-width: 0;
      &:last-child{
        border-right-width: 1px;
      }

      i{
        ._icon(18);
        position: relative;
        top: -2px;
        margin-right: 0.25em;
        vertical-align: middle;
      }

      &.dot { i { background-image: url(../images/icon_dot.png); } }
      &.line{ i { background-image: url(../images/icon_line.png); } }
      &.area{ i { background-image: url(../images/icon_area.png); } }
      &.del { i { background-image: url(../images/icon_del.png); } }

      &:hover,&.cur {
        background-color: @cl_blue;
        //border-color: #fff;
        color: #ffffff;
        &.dot { i { background-image: url(../images/icon_dot_white.png); } }
        &.line{ i { background-image: url(../images/icon_line_white.png); } }
        &.area{ i { background-image: url(../images/icon_area_white.png); } }
        &.del { i { background-image: url(../images/icon_del_white.png); } }
      }

    }
    &.Point{
      a.line,a.area{ display: none;}
    }
    &.LineString{
      a.dot,a.area{ display: none;}
    }
    &.Polygon{
      a.line,a.dot{ display: none;}
    }

    &.forbid{
      a.dot,a.line,a.area{ display: none;}
      &.and_clear{
        a.del{
          display: none;
        }
      }
    }
  }

}

.ol-popup-nowrap{
  background-color: #ffffff;
}
