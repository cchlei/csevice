//返回顶部
.back_top {
	._size(100);
	&:extend(.__icon);
	position: fixed;
	@g:30px;
	bottom: @g;
	right: @g;
	background-image: url(../images/icon_backtop_qt.png);
	z-index: 999;
	background-color: #37a7fa;
	//animation: flipInX 0.6s 1 linear 0.6s;
	animation-fill-mode:both;
	border-radius: 50%;
	.dura();

	display: none;
	transition: bottom 0.9s cubic-bezier(0.57, 0, 0.3, 0.99),margin-bottom 0.3s cubic-bezier(0.57, 0, 0.3, 0.99);

	&:hover {
		cursor: pointer;
		margin-bottom: 12px;
		box-shadow: #fff 0 0 9px;
	}
	.fp-viewing-0 &{
		//animation-name: flipOutX;
		bottom: -120px;
	}
}


@keyframes rotateme {
	0%{
		transform: rotate(0deg);
	}
	100%{
		transform: rotate(360deg);
	}
}

.section.active{
	.fp-tableCell{
		//当前的标题
		>h3{
			animation: pulse 0.9s 1 linear ;
			animation-fill-mode:both;
		}
	}

}