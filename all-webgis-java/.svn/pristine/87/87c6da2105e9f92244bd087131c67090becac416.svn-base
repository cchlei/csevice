    }
    .contain.fp-tableCell h3 i{
        background: url(images/page3_2.png) no-repeat;
        height: 24px;
        display: block;
        position: absolute;
        left: 50%;
        top: 150px;
        width: 679px;
        margin-left: -339.5px;
    }
    #sec4 .effect-container{width:1200px;top:30%;left:50%;margin-left:-600px;text-align:center;position:absolute}
    #sec4 .effect-container ul li{width:33.3%;margin:0 auto;text-align:center;float:left}
    #sec4 .effect-container ul li span{font-size:24px;font-family:"微软雅黑";color:#6b6b6b}
    #sec4 .gdhy{position:absolute;bottom:10%;left:50%;width:1200px;margin-left:-600px;text-align:center}
    #sec4 .gdhy p{border-radius:50px;-moz-border-radius:50px;-ms-border-radius:50px;-o-border-radius:50px;-webkit-border-radius:50px;width:170px;height:46px;line-height:46px;border:1px solid #000;margin:0 auto}
    #sec4 .gdhy p a{color:#868383}

    .page1 {
        background: url(images/ie_page1.png) 50% 50% no-repeat;
    }
    .page2 {
        background: url(images/ie_page2.png) 50% 50% no-repeat;
    }
    .page3 {
        background: url(images/ie_page3.png) 50% 50% no-repeat;
    }
    .page4 {
        background: url(images/ie_page4.png) 50% 50% no-repeat;
    }
    .page5 {
        background: url(images/ie_page5.png) 50% 50% no-repeat;
    }
    #navBar {
        z-index: 3;
        position: absolute;
        top: 50%;
        right: 3%;
        height: 250px;
        margin-top: -125px;
    }
    #navBar .active {
        width: 12px;
        height: 12px;
        position: relative;
        left: 0px;
    }
    #navBar li {
        cursor: pointer;
        margin-bottom: 10px;
        transition: all .7s ease;
        border-radius: 50%;
        line-height: 40px;
        text-align: center;
        width: 6px;
        height: 6px;
        background-color: #333;
        position: relative;
        left: 3px;
    }
    .cr{
        position: fixed;
        bottom: 24px;
        color: #fff;
        font-size: 15px;
        font-family:"微软雅黑";
        width:600px;
        text-align: center;
        margin-left: -300px;
        left: 50%;
    }
    .cr a{
        color: #fff;
        margin-right: 6px;
    }
    .cr p{
        margin-top: 6px;
    }
    .cr .weibo i{
        display: inline-block;
        vertical-align: middle;
        width: 20px;
        height: 20px;
        background-image: url(images/icon_cotact.png);
        margin-right: .5em;
        margin-bottom: -2px;
        top: 5px;
        background-position: 0% -58px;
    }
    .qq i{
        display: inline-block;
        vertical-align: middle;
        width: 20px;
        height: 20px;
        background-image: url(images/icon_cotact.png);
        margin-right: .5em;
        margin-bottom: -2px;
        top: 5px;
        background-position: 50% -27px;
        position: relative;
        top: -3px;
    }
    .mail i{
        background-position: 50% 0;
        display: inline-block;
        vertical-align: middle;
        width: 20px;
        height: 20px;
        background-image: url(images/icon_cotact.png);
        margin-right: .5em;
        margin-bottom: -2px;
        top: 5px;
    }


</style>
