//重置密码
.repassword{
    width:1000px;
    margin:0px auto;
    .pos(r);
    .font_wei;
    .zccenter{
        color:#787777;
        width:500px;
        .pos(a);
        height:240px;
        top:50%;
        left:14px;
        margin-top:-120px;
        z-index: 99;
        h3{
           .font_s(18px);
           height:40px;
           font-weight: bold; 
           span{
            color:#6480b4;
           }
        }
        p{
            line-height:35px;
            .font_s(14px);
            width:500px;
            word-wrap:break-word;
            a{
                color:#e54419;
                .font_s(18px);
                text-decoration:underline;
                font-weight: bold; 
            }
            &.link{
                color:#308bee;
            }
        }
    }
    img{
        width:472px;
        height:241px;
        .pos(a);
        left:500px;
        top:60%;
        margin-top:-120px;
    }
}
//重置密码表单
.resetpassword{
    .topp{
        width:804px;
        height:58px;
        .pos(f);
        left:50%;
        margin-left:-402px;
        top:25%;
        margin-top:-30px;
        background:url(../images/resetpass.jpg) no-repeat center;
    }
    .zccenter{
        width:450px;
        height:260px;
        .pos(f);
        left:50%;
        margin-left:-225px;
        top:70%;
        margin-top:-130px;
        z-index: 99;
        li{
            .font_s(14px);
            .font_wei;
            height:80px;
            line-height:30px;
            span{
                width:130px;
                display: inline-block;
                color:#4d4d4d;
                text-align:right;
            }
            input{
                width:210px;
                margin-left:10px;
                padding-left:5px;
                height:35px;
                line-height:35px;
            }
            &.ascertain{
                width:154px;
                color:#fff;
                background-color:#3f92e8;
                height:40px;
                line-height:40px;
                text-align:center;
                margin: 0px auto;
                margin-top:10px;
                cursor: pointer;
            }
        }
    }
}
.cloud{
    i.cloud1{
        .pos(f);
        top:30%;
        width:188px;
        height:71px;
        margin-top:-35.5px;
        left:70%;
        margin-left:-94px;
        background:url(../images/p_cloud1.png) no-repeat center;
    }
    i.cloud2{
        .pos(f);
        top:20%;
        width:91px;
        height:38px;
        margin-top:-19px;
        left:33%;
        margin-left:-45px;
        background:url(../images/p_cloud2.png) no-repeat center;
    }
}


.errtip {
    color: #f00 !important;
    text-indent: 10em;
    display: inline;
    line-height: 2em;
    display: inline-block !important;
    width:450px !important;
    text-align: inherit !important;
    /* margin-top: -8em; */
}




.fasong{
    background: #6487d2;
    padding: 3px 8px;
    text-align: center;
    color: #fff!important;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    cursor: pointer;
}
.tishi{
    em{
        color: #4c78d5;
        margin-right: 20px;
    }
    .ts{
        height: 72px!important;
        line-height: 26px;
    }
    a.re{
        background: #4c78d5;
        padding: 3px 8px;
        text-align: center;
        color: #fff !important;
        border-radius: 5px;
        -webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        margin-left: 20px;
    }
}
