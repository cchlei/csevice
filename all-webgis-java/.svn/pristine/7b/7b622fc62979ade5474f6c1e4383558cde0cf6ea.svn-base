/*ÌáÊ¾Ò³Ãæ*/
.font_wei{
    font-family: "Microsoft YaHei";
}
.font_song{
    font-family: "SimSun";
}
.font_s(@size){
    font-size:@size;
}
.pos(r){
    position:relative;
}
.pos(a){
    position:absolute;
}
.pos(f){
    position:fixed;
}
.aa{
    width:100%;
    height:100%;
    overflow: hidden;
}
.top{
    height:100px;
    line-height:100px;
    background-color:#3f92e8;
    padding-left:170px;
    i{
        width:449px;
        height:80px;
        display:inline-block;
        vertical-align:middle;
        background:url(../images/p_logo.png) no-repeat center;
        margin-right:20px;
    }
}




#prompt_center{
    width:1000px;
    margin:0px auto;
}

.foot{
    width:100%;
    margin:0px auto;
    .pos(f);
    bottom:0px;
    text-align: center;
    height:115px;
    .font_s(12px);
    ul{
        margin:0px 70px;
        height:35px;
        line-height:35px;
        border:1px solid #d8d7d6;
        border-left:0px;
        border-right:0px;
        li{
            display:inline-block;
            .font_wei;
            color:#7f8082;
        }
    }
    a{
        color:#7f8082;
        padding:0px 25px;
    }
    p{
        height:55px;
        line-height:55px;
        color:#898888;
        .font_song;
    }
}
//注册成功邮箱激活
.zc_success{
    width:1000px;
    margin: 0px auto;  
    .pos(r);  
    img{
        width:377px;
        height:256px;
        .pos(a);
        left:560px;
        bottom:40%;
        margin-bottom:-178px;
        
    }
    .zccenter{
        width:500px;
        .font_s(14px);
        .font_wei;
        .pos(r);
        height:120px;
        left:60px;
        top:50%;
        margin-top:-60px;
        z-index: 99;
        i{
            width:45px;
            height:150px;
            background:url(../images/p_success.png) no-repeat;
            float:left;
            margin-right:35px;
        }
        .tishi{
            h3{
                color:#6480b4;
                .font_s(18px);
                line-height:40px;
                text-shadow:2px 1px 0px #D1D1D1;
                -moz-text-shadow:2px 1px 0px #D1D1D1;
                -webkit-text-shadow:2px 1px 0px #D1D1D1;
            }
            p{
                color:#787777;
                .font_s(14px);
                line-height:30px;
                text-shadow:2px 1px 0px #E6E6E6;
                -moz-text-shadow:2px 1px 0px #E6E6E6;
                -webkit-text-shadow:2px 1px 0px #E6E6E6;
                a{
                    color:#3f92e8;
                    font-weight: bold;
                }
            }
            
        }
        span{
            color:#888889;
            font-weight: bold;
        }
    }
}
#procenter{
    width:1000px;
    margin:0px auto;
}

//注册失败
.zc_fail{
    width:1000px;
    margin: 0px auto;  
    .pos(r); 
    img{
        height:384px;
        .pos(a);
        left:0px;
        bottom:50%;
        margin-bottom:-192px;
    }
    .zccenter{
        width:500px;
        .font_s(14px);
        .font_wei;
        .pos(a);
        height:190px;
        left:470px;
        top:50%;
        margin-top:-95px;
        z-index: 99;
        h3{
            color:#6480b4;
            .font_s(18px);
            height:60px;
            }
            p{
                color:#787777;
                .font_s(14px);
                line-height:30px;
                &.yh{
                    color:#3f92e8;
                }
                &.hy{
                    color:#93a2b9;
                }
                &.again{
                 margin-top:28px;
                }
                a{
                    color:#7e7c7c;
                    text-decoration:underline;
                }
                span{
                    color:#fff;
                    background-color:#56c793;
                    padding:12px 45px;
                    margin-right:70px;
                }
            }
    }
}

//邮箱激活
.email_activation{
    width:1000px;
    .font_s(14px);
    .font_wei;
    margin:0px auto;
    .pos(r);
    img{
        width:224px;
        height:252px;
        .pos(a);
        left:20%;
        margin-left:-112px;
        top:50%;
        margin-top:-176px;

    }
    .zccenter{
        width:500px;
        .font_s(14px);
        .font_wei;
        .pos(a);
        height:190px;
        left:65%;
        margin-left:-250px;
        top:50%;
        margin-top:-95px;
        z-index: 99;
        color:#6480b4;
        h3{
            .font_s(18px);
            height:50px;
            font-weight: 100;
            }
            p{
                .font_s(14px);
                line-height:30px;
                &.again{
                 margin-top:28px;
                }
