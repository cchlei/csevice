@import "tool";
.toolClass;

body{
  min-width: 100px;
}
.font_wei{
  font-family: "Microsoft YaHei";
}
.font_song{
  font-family: "SimSun";
}
.font_s(@size){
  font-size:@size;
}
.pos(r){
  position:relative;
}
.pos(a){
  position:absolute;
}
.pos(f){
  position:fixed;
}
.clear{
  clear: both;
}
textarea{
  outline:none;
  resize:none;
}
.navright{
  .pos(r);
  .font_wei;
  color:#333;
  .fwtop{
    height:115px;
    line-height:115px;
    background-color:#eaedf2;
    .pos(r);
    top:0px;
    width:100%;
    z-index: 999;
    em{
      .pos(a);
      .font_s(28px);
      top: 0px;
      left:33px;
    }
    span{
      height:115px;
      line-height:115px;
      .pos(a);
      top: 0px;
      right:33px;
      display:block;
      font-size:14px;
      cursor:pointer;
    }
  }
  .map_make{
    .font_wei;
    margin: 20px 28px 0;
    .mapmake_search{
      background-color: #f0f3f8;
      padding: 20px;

      .makemap{
        .font_s(14px);
        .font_wei;
        color: #4c78d5;
        float: left;
        position: relative;
        top: 2px;
        height: 44px;
        line-height: 44px;
        a{
          color: #4c78d5;
        }
        i{
          width: 44px;
          height: 44px;
          background: url("../images/map_make.png") no-repeat;
          display: inline-block;
          vertical-align: middle;
          margin-right: 10px;
        }
      }
      .fbtime select{
        background-color:#f0f3f8;
        border: 0px;
        .font_s(15px);
        outline: none;
      }
      .map_search_top{
        width: 675px;
        color: #4c78d5;
        .font_s(20px);
        .pos(r);
        float: right;
        .map_search{
          width: 140px;
          height: 44px;
          line-height: 44px;
          text-align: center;
          color: #fff;
          .font_s(16px);
          background-color: #4c78d5;
          .pos(a);
          right: 0px;
          top: 0px;
          cursor: pointer;
        }
        .map_value{
          width: 552px;
          height: 40px;
          margin-left: 15px;
          border: 1px #4c78d5 solid;
          text-indent: 1em;
          .font_s(16px);
          background-color: #f0f3f8;
          .pos(a);
          right:0px;
        }
      }
    }
    .releasemap{
      .font_wei;
      margin-top: 20px;
      ul.grid{
        width: 100%;
        height: 100%;
        li{
          width: 23.5%;
          height: 50%;
          float: left;
          text-align: center;
          margin-right: 2%;
          cursor:pointer;
          .pos(r);
          .map_bg{
            .pos(a);
            top: 0px;
            width: 100%;
            height: 85%;
            background-color: #14212f;
            opacity: 0.6;
            display: none;
          }
          .mapoperation{
            .pos(a);
            top: 0px;
            width: 100%;
            height: 85%;
            .font_wei;
            .font_s(16px);
            color: #fff;
            overflow: hidden;
            p{
              .pos(a);
              top: 8px;
              left: -37px;
              text-align: center;
              padding: 7px;
              width: 129px;
              height: 33px;
              line-height: 33px;
              background-color: #e7e9ea;
              color: #4c78d5;
              transform: rotate(-43deg);
              -ms-transform: rotate(-43deg);
              -moz-transform: rotate(-43deg);
              -webkit-transform: rotate(-43deg);
              -o-transform: rotate(-43deg);
              &.blue{
                background-color: #ef6659;
                color: #fff;
              }
              &.orang{
                background-color: #7597e0;
                color: #fff;
              }
            }
            a{
              color: #fff;
              display: block;
              &:extend(.expandin);
            }
            ul{
              width: 225px;
              .pos(a);
              bottom: 15px;
              left: 50%;
              margin-left: -112.5px;
              display: none;
            }
            li{
              width: 55px;
              height: 55px;
              line-height: 55px;
              border-radius: 65px;
              -moz-border-radius: 65px;
              -webkit-border-radius: 65px;
              -o-border-radius:65px;
              opacity: 1;
              z-index: 99;
              margin: 0px 10px;
              &.fb.ed,&.bj.ed,&.sc.ed{
                .font_s(16px);
                background: #4c78d5;
              }
              &.fb{
                background: url("../images/m_fb.png") no-repeat #4c78d5 center;
                display: inline-block;
                .font_s(0px);
              }
              &.bj{
                background: url("../images/m_bj.png") no-repeat #4c78d5 center;
                display: inline-block;
                .font_s(0px);
              }
              &.sc{
                background: url("../images/m_sc.png") no-repeat #4c78d5 center;
                display: inline-block;
                .font_s(0px);
              }
            }
          }
          img{
            width: 100%;
            height: 85%;
            margin: 0px auto;
          }
          span{
            text-align: center;
            display: block;
            .font_s(14px);
            .pos(a);
            bottom:17px;
            .txcut;
            width: 100%;


          }
          &:nth-of-type(4),&:nth-of-type(8){
            margin-right: 0px;
          }
          &:hover{
            .map_bg,.mapoperation>ul{
              display: block;
            }

          }
        }
      }
    }
    .page{
      width: 645px;
      margin: 10px auto;
      /* .pos(f);
       left:50%;
       margin-left: -322.5px;
       bottom: 10px;*/
      &.compact-theme .current{
        color: #dde6f8;
        background: #235dd7;
        background: -webkit-linear-gradient(top,#235dd7 0,#235dd7 100%);
      }
      &.compact-theme a, &.compact-theme span{
        border: 0px solid #839ACA;
        box-shadow: 0px 0px 0px rgba(0,0,0,.2);
        background: #4C78D5;
        color: #fff;
        line-height: 30px;
      }
      &.compact-theme .ellipse{
        background: #4c78d5;
        color: #fff;
      }
      &.compact-theme a:hover{
        background:#4C78D5;
        background:-webkit-gradient(linear,left top,left bottom,color-stop(0,#4C78D5),color-stop(100%,#4C78D5));
        color: #fff;
      }
      li{
        margin: 0px 5px;
        border: 0px;
        span.current.prev,a.page-link.prev{
          background:url("../images/left_jt.png") no-repeat center #4C78D5;
          width: 24px;
          height: 30px;
          .font_s(0px);
          .font_s(0px);
          text-align: center;
        }
        a.page-link.next,span.current.next{
          background:url("../images/right_jt.png") no-repeat center #4C78D5;
          width: 24px;
          height: 30px;
          .font_s(0px);
          text-align: center;
        }
      }

    }
  }

}

@import "tool";

html body{
  .selectBox{
    .font_s(14px);
    .font_wei;
    background: none;
    border: none;
    .lineh(44px);
    position: relative;
    min-width: 80px;
    -webkit-box-shadow: 0 0px 0 rgba(255, 255, 255, .75);
    .selectBox-label{
      font-size: 14px;
      padding: 0 1em;
    }
    .selectBox-arrow{
      background: none;
      width: 0;
      height: 0;
      border: none !important;
      .triangle(5px,bottom,#666) !important;
      position: absolute;
      top: 50%;
      margin-top: -2.5px;
    }
  }
  .selectBox-dropdown-menu{
    border: 1px solid #666 !important;
    border-radius: 0;
    margin-left: 1em;
    li{
      a{
        line-height: 36px;
        height: 36px;
        .font_s(14px);
        .font_wei;
      }

    }
  }
}


.map_creater_form{
  font-size: 14px;
  padding: 1em 3em;
  .yahei;

  border-top:6px solid #4C78D5;
  border-bottom:6px solid #4C78D5;

  h2{
    text-align: center;
    font-size: 24px;
    color: #4C78D5;
    font-weight: 100;
    line-height: 2em;
  }

  p{
    display: block;
    line-height:3em;
    margin:1em 3em;
    white-space: nowrap;
    overflow:visible;
    em{
      display: inline-block;
      vertical-align: top;
      font-size: 14px;
      line-height: 2em;
      min-width: 6em;
    }


    input,textarea{
      width: 21em;
      vertical-align: top;
      line-height: 2em;

    }

    input{

    }

    textarea{
      height: 6em;
    }
  }

  .btn{
    display: inline-block;
    vertical-align: top;
    background-color: #4C78D5;
    color: #ffffff;
    line-height: 3em;
    text-align: center;
    cursor: pointer;
    .padding_h(3em);
    position: relative;
    left: 4px;

  }
}